    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/datatables.min.js"></script>

    <script src="~/DataTables-1.10.21/js/dataTables.bootstrap4.min.js"></script>
