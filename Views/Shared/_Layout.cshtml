                                    <a class="dropdown-item dropdown-toggle"><img src="~/images/icons/icons8-usuário-de-gênero-neutro-30 (1).png" /> Usuario</a>
                                    <ul class="dropdown-menu">
                                        <li><a class="dropdown-item" asp-area="" asp-controller="PerfilUsuarios" asp-action="Index">Perfil Usuario</a></li>
                                        <li><a class="dropdown-item" asp-area="" asp-controller="AcessoTipoUsuarios" asp-action="Index">Tipo Acesso</a></li>
                                        <li><a class="dropdown-item" asp-area="" asp-controller="TipoUsuarios" asp-action="Index">Tipo Usuário</a></li>

                                        @*<li class="dropdown-submenu">
                                                <a class="dropdown-item dropdown-toggle" href="#">Subsubmenu</a>
                                                <ul class="dropdown-menu">
                                                    <li><a class="dropdown-item" href="#">Subsubmenu action</a></li>
                                                    <li><a class="dropdown-item" href="#">Another subsubmenu action</a></li>
                                                </ul>
                                            </li>
                                            <li class="dropdown-submenu">
                                                <a class="dropdown-item dropdown-toggle" href="#">Second subsubmenu</a>
                                                <ul class="dropdown-menu">
                                                    <li><a class="dropdown-item" href="#">Subsubmenu action</a></li>
                                                    <li><a class="dropdown-item" href="#">Another subsubmenu action</a></li>
                                                </ul>
                                            </li>*@



                                    </ul>
                                </li>
                            </ul>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Locação
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                                <li><a class="dropdown-item" asp-area="" asp-controller="LocaEquipamentoes" asp-action="Index">Loc. Equipamentos</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="LocaSalas" asp-action="Index">Loc. Salas</a></li>
                                <li><a class="dropdown-item" asp-area="" asp-controller="Devolucaos" asp-action="Index">Devolução</a></li>
                            </ul>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Relatorios" asp-action="Index">Relatórios</a>
                        </li>

                    </ul>










                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @*<footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - GestaoS - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/BuscaListaSalaPorFilial.js"></script>
    <script src="~/js/QtdeEquipamentoDisponiveis.js"></script>
